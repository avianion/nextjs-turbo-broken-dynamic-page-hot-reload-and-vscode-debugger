  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker Nextjs Debug",
      "type": "node",
      "request": "attach",
      // "address": "127.0.0.1",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/app",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "port": 9230,
      "sourceMapPathOverrides": {
        "/turbopack/[project]/*": "${webRoot}/nextjs/*"
      }
    }
  ]
}
